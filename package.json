{
  "name": "tractor-server",
  "version": "0.10.0-beta.0",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-server.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-server/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/index.js",
  "watch": {
    "lint": "src/**/*.js",
    "test": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile",
    "compile": "babel src/ --out-dir dist --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "dedent": "0.7.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.14.1",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.2.0",
    "npm-watch": "0.1.7",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "bluebird": "3.4.7",
    "body-parser": "1.16.0",
    "camel-case": "3.0.0",
    "cors": "2.8.1",
    "escodegen": "1.8.1",
    "esprima": "2.7.3",
    "esquery": "0.4.0",
    "estemplate": "0.5.1",
    "express": "4.14.0",
    "gherkin": "2.12.2",
    "lodash.escaperegexp": "4.1.2",
    "lodash.isobject": "3.0.2",
    "lodash.last": "3.0.0",
    "lodash.template": "4.4.0",
    "npmlog": "4.0.2",
    "pascal-case": "2.0.0",
    "socket.io": "1.7.2",
    "stripcolorcodes": "0.1.0",
    "tractor-client": "0.10.0-beta.0",
    "tractor-config-loader": "0.1.0",
    "tractor-error-handler": "0.1.2",
    "tractor-file-structure": "0.1.0-beta.5",
    "tractor-js-file-transformer": "0.1.0",
    "tractor-logger": "0.1.0-beta.0",
    "tractor-plugin-loader": "0.1.2"
  }
}
